      div.section__account_softphone-content_dial-keyLine
        +boardKeyItem(1, '')
        +boardKeyItem(4, 'ghi')
        +boardKeyItem(7, 'pqrs')
        +boardKeyItem('', '*', 'section__account_softphone-content_dial-keyStar')
      div.section__account_softphone-content_dial-keyLine
        +boardKeyItem(2, 'abc')
        +boardKeyItem(5, 'jkl')
        +boardKeyItem(8, 'tuv')
        +boardKeyItem(0, '+')
      div.section__account_softphone-content_dial-keyLine
        +boardKeyItem(3, 'def')
        +boardKeyItem(6, 'mno')
        +boardKeyItem(9, 'wxyz')
        +boardKeyItem('', '#', 'section__account_softphone-content_dial-keyCell')
    div.section__account_softphone-content_dial-phoneCall
