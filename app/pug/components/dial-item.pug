        p.section__account_softphone-content_dial-accName Account_1
        p.section__account_softphone-content_dial-accNumber +1(789)123-44-44
      div.section__account_softphone-content_dial-accListItem
        p.section__account_softphone-content_dial-accName Account_2
        p.section__account_softphone-content_dial-accNumber +1(654)157-12-35
      div.section__account_softphone-content_dial-accListItem
        p.section__account_softphone-content_dial-accName Account_3
        p.section__account_softphone-content_dial-accNumber +1(234)567-89-00

  div.section__account_softphone-content_dial-keyboard
